{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "attach to vv",
      "type": "cppdbg",
      "request": "attach",
      "program": "${workspaceFolder}/bin/vv",
      "MIMode": "lldb"
    },
    {
      "name": "nvim quit",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/vv",
      "args": [
        "nvim --clean --cmd ':qa'"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
